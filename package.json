{
  "name": "docsy",
  "displayName": "Docsy",
  "version": "0.4.1",
  "description": "Support for Docsy syntax",
  "categories": [
    "Programming Languages"
  ],
  "homepage": "https://github.com/etienne-dldc/vscode-docsy#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/vscode-docsy/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/vscode-docsy.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "publisher": "etienne-dldc",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "contributes": {
    "grammars": [
      {
        "language": "docsy",
        "scopeName": "source.docsy",
        "path": "./syntaxes/docsy.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "docsy",
        "aliases": [
          "Docsy",
          "docsy"
        ],
        "extensions": [
          ".docsy",
          ".dy"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "vsce": "^2.7.0"
  },
  "engines": {
    "vscode": "^1.42.0"
  }
}
