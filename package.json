{
  "name": "docsy",
  "displayName": "Docsy",
  "description": "Support for Docsy syntax",
  "version": "0.3.0",
  "publisher": "etienne-dldc",
  "repository": "https://github.com/etienne-dldc/vscode-docsy",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "docsy",
        "aliases": [
          "Docsy",
          "docsy"
        ],
        "extensions": [
          ".docsy",
          ".dy"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "docsy",
        "scopeName": "source.docsy",
        "path": "./syntaxes/docsy.tmLanguage.json"
      }
    ]
  }
}
