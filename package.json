{
    "name": "docsy",
    "displayName": "Docsy",
    "description": "Support for Docsy syntax",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "docsy",
            "aliases": ["Docsy", "docsy"],
            "extensions": [".docsy"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "docsy",
            "scopeName": "source.docsy",
            "path": "./syntaxes/docsy.tmLanguage.json"
        }]
    }
}